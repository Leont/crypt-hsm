	CK_MECHANISM_TYPE       T_MECHANISM_TYPE
	Attributes              T_ATTRIBUTES
	Session_flags           T_SESSION_FLAGS

INPUT
T_USER_TYPE
	$var = get_user_type($arg)

T_MECHANISM_TYPE
	$var = get_mechanism_type($arg)

T_ATTRIBUTES
	$var = get_attributes($arg)

T_SESSION_FLAGS
	$var = get_flags(session_flags, $arg)
